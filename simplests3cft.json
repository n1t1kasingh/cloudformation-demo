{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "S3Bucket": {
            "Type": "AWS::S3::Bucket"
        },
        "Streams": {
            "Type": "AWS::Kinesis::Stream",
            "Properties": {
                "Name": "KinesisreqName", 
                "RetentionPeriodHours" : 168, 
                "ShardCount": 4,
                "StreamEncryption": { 
                    "EncryptionType": "KMS", 
                    "KeyId": "!Ref myKey" 
                    }
                } 
        }
    },
    "Outputs": {
        "BucketName": {
            "Value": {
                "Ref": "S3Bucket"
            },
            "Description": "Name of the sample Amazon S3 bucket."
        },
        "StreamName": {
            "Value": {
                "Ref": "Streams"
            },
           "Description": "Name of the sample Kinesis Stream."
    }
}
}
